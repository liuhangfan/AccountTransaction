    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>RELEASE</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <name>Archetype - AccountTransaction</name>
  <url>http://maven.apache.org</url>
